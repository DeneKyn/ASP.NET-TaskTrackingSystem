@model TaskTrackingSystem.Models.Project;

<form asp-action="create" asp-controller="project">
    <div class="form-group">
        <label asp-for="Name" class="control-label">Название</label>
        <input type="text" asp-for="Name" class="form-control" />
    </div>
    
    <div class="form-group">
        <input type="submit" asp-route-username="@ViewBag.Username" value="Send" class="btn btn-default" />
    </div>
</form>